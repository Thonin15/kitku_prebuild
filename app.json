{
	"expo": {
		"name": "kitku",
		"slug": "kitku",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "kitku",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash2.png",
			"resizeMode": "contain",
			"backgroundColor": "#4B7F52"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.kitku.app",
			"config": {
				"googleMapsApiKey": "AIzaSyAkh4byBaWG7-HOg_6EcHZfA9JVz-_ZfpQ"
			}
		},
		"android": {
			"package": "com.kitku",
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"config": {
				"googleMaps": {
					"apiKey": "AIzaSyAkh4byBaWG7-HOg_6EcHZfA9JVz-_ZfpQ"
				}
			},
			"permissions": [
				"ACCESS_FINE_LOCATION",
				"ACCESS_COARSE_LOCATION",
				"INTERNET"
			]
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "576f3f83-10e5-403e-91a4-aaac9e81ae83"
			}
		},
		"owner": "thonin"
	}
}
